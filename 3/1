using System;

class Program
{
    static void Main()
    {
        int a;
        string[] d = new string[] { "Saturday", "Sunday", "Monday", "Tuesday", "Wednesday", "Thursday", "Friday" };
        Console.Write("enter number: ");
        a = int.Parse(Console.ReadLine());
        if (a >= 1 && a <= 7)
        {
            Console.WriteLine(d[a - 1]);
        }
        else
        {
            Console.WriteLine("end!");
        }
        Console.ReadKey();
    }
}
